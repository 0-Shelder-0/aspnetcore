    "Microsoft.Extensions.DependencyInjection": "1.0.0-*",
    "Microsoft.Extensions.PlatformAbstractions.Dnx": "1.0.0-*",
    "System.Collections": "4.0.11-*",
    "System.IO": "4.1.0-*",
    "System.Reflection": "4.1.0-*",
    "System.Threading": "4.0.11-*"
  },
  "frameworks": {
    "net451": {},
    "netstandard1.5": {
      "imports": [
        "portable-net451+win8",
        "dotnet5.6"
      ]
    }
  }
}