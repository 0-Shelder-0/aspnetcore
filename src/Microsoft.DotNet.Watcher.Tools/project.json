{
  "version": "1.0.0-*",
  "buildOptions": {
    "outputName": "dotnet-watch",
    "warningsAsErrors": true,
    "emitEntryPoint": true,
    "debugType": "portable",
    "keyFile": "../../tools/Key.snk",
    "copyToOutput": "toolassets/*.targets",
    "embed": {
      "mappings": {
        "dotnetwatch.targets": "dotnetwatch.targets"
      }
    },
    "compile": {
      "include": "../Shared/**/*.cs"
    }
  },
  "publishOptions": {
    "include": [
      "toolassets/*.targets",
      "prefercliruntime"
    ]
  },
  "dependencies": {
    "Microsoft.Extensions.Process.Sources": {
      "type": "build",
      "version": "1.0.0"
    },
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.0"
    }
  },
  "frameworks": {
    "netcoreapp1.0": {}
  }
}