{
  "version": "1.0.0-*",
  "buildOptions": {
    "outputName": "dotnet-user-secrets",
    "emitEntryPoint": true,
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "copyToOutput": "toolassets/*.targets",
    "compile": {
      "include": "../Shared/**/*.cs"
    }
  },
  "publishOptions": {
    "include": [
      "toolassets/*.targets",
      "prefercliruntime"
    ]
  },
  "dependencies": {
    "Microsoft.Extensions.Configuration.UserSecrets": "1.0.0",
    "Microsoft.NETCore.App": {
      "version": "1.0.0",
      "type": "platform"
    }
  },
  "frameworks": {
    "netcoreapp1.0": {}
  }
}