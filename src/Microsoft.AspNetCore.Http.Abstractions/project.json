        "System.ComponentModel": "4.0.0-*",
        "System.Globalization.Extensions": "4.0.0-*",
        "System.Linq.Expressions": "4.0.10-*",
        "System.Net.WebSockets": "4.0.0-*",
        "System.Reflection.TypeExtensions": "4.0.0-*",
